{
  "functions": {
    "api/calendar/[token].js": {
      "maxDuration": 60
    },
    "api/admin/index.js": {
      "maxDuration": 10
    }
  },
  "rewrites": [
    { "source": "/", "destination": "/api" },
    { "source": "/admin", "destination": "/api/admin" },
    { "source": "/health", "destination": "/api/health" },
    { "source": "/cal/:token/calendar.ics", "destination": "/api/calendar/:token" }
  ]
}
